fun areWeHavingItYet() {
  print "Yes we are!";
}

print areWeHavingItYet;

fun repeat(fn, n) {
  print "repeat after me!";
  for (var i = 0; i < n; i = i + 1) {
    fn();
  }
}

repeat(areWeHavingItYet, 3);

fun sum(a, b, c) {
  return a + b + c;
}

print sum(1, 2, 3);

fun fib(n) {
  if (n < 2) {
    return n;
  }

  return fib(n - 2) + fib(n - 1);
}

print fib(35);

